// recursion
search1 = fn(list, item, search2) (
  if( not exists(list) ) null
  else (
    n = count(list),
    m = long(n / 2),
    c = compare(item, list[ m ]),
    if     ( c < 0 ) search2( list[*:m-1], item, search2 )
    else if( c > 0 ) 1 + m + search2( list[m+1:*], item, search2 )
    else             m
  )
);
